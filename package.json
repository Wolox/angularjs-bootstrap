{
  "name": "frontend-bootstrap",
  "version": "0.0.1",
  "engines": {
    "node": ">= 10.15.3",
    "npm": ">= 6.9.0"
  },
  "description": "Kickoff for web applications",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js",
    "test": "karma start test/unit/karma.conf.js",
    "protractor": "protractor test/e2e/protractor.conf.js",
    "protractor-local": "protractor --baseUrl='http://localhost:3000/' test/e2e/protractor.conf.js",
    "code-quality-html-report": "plato -r -d code-quality-report -e .eslintrc -x build/js/vendor build/js",
    "code-quality-json-report": "cr -o code-quality-report/stats.json -f json -x build/js/vendor build/js",
    "style-lint": "find ./src -name \\**.scss | xargs ./node_modules/.bin/stylelint --config .stylelintrc --syntax scss",
    "reports": "npm run code-quality-html-report && npm run code-quality-json-report",
    "build": "webpack --config webpack.config.js"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Wolox/frontend-bootstrap.git"
  },
  "author": "Sebastian Balay <sebastian.balay@wolox.com.ar>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wolox/frontend-bootstrap/issues"
  },
  "homepage": "https://github.com/Wolox/frontend-bootstrap",
  "dependencies": {
    "@uirouter/angularjs": "^1.0.21",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "angular": "^1.5.8",
    "angular-local-storage": "^0.5.2",
    "angular-mocks": "^1.7.6",
    "angular-route": "^1.7.7",
    "angular-ui-router": "^1.0.20",
    "autoprefixer": "^9.4.6",
    "clean-webpack-plugin": "^1.0.1",
    "domino": "^2.1.1",
    "dotenv-webpack": "^1.7.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lit-element": "^2.4.0",
    "stylelint": "^13.9.0",
    "stylelint-config-wolox": "^1.0.5",
    "stylelint-no-indistinguishable-colors": "^1.2.1",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^3.5.3",
    "wolox-equalizer": "0.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/helpers": "^7.12.5",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/runtime": "^7.12.5",
    "angular-translate": "^2.18.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "backstopjs": "^3.9.2",
    "browser-sync": "^2.26.3",
    "complexity-report": "^2.0.0-alpha",
    "concurrent-transform": "^1.0.0",
    "connect-history-api-fallback": "^1.6.0",
    "css-loader": "^2.1.1",
    "del": "^2.2.0",
    "eslint": "^7.18.0",
    "eslint-config-angular": "^0.5.0",
    "eslint-plugin-angular": "^1.0.1",
    "eslint-webpack-plugin": "^2.4.3",
    "html-loader": "^0.5.5",
    "jasmine-core": "^2.4.1",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "^3.2.0",
    "karma": "^4.0.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "^1.1.2",
    "karma-junit-reporter": "^1.0.0",
    "karma-ng-html2js-preprocessor": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "merge-stream": "^1.0.1",
    "minimatch": "^3.0.4",
    "mkdirp": "^0.5.1",
    "node-notifier": "^8.0.1",
    "node-sass": "^4.11.0",
    "phantomjs-prebuilt": "^2.1.7",
    "plato": "^1.5.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "prompt": "^1.0.0",
    "protractor": "^5.4.2",
    "replace": "^1.0.1",
    "require-dir": "^0.3.2",
    "run-sequence": "^1.1.5",
    "sass-loader": "^7.1.0",
    "stream-series": "^0.1.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.4.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.2",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.2.1",
    "webpack-monitor": "^1.0.14",
    "webpack-serve": "^0.3.1",
    "yargs": "^6.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run style-lint"
    }
  }
}
